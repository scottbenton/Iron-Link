  "scripts": {
    "dev": "vite",
    "tsc": "tsc -b",
    "build": "npm run tsc && vitest --run && vite build",
    "lint": "eslint .",
    "format": "eslint . --fix && prettier . --write",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "extract:i18n": "i18next 'src/**/*.{ts,tsx}' [-oc]",
